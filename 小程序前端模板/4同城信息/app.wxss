.bt_list {
  height: 80rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  overflow: hidden;
  background: #fff;
  border-top: 1rpx solid #e2e2e2;
}

.bt_list view {
  width: 50%;
  float: left;
  border-radius: 0rpx;
  border: 0rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  height: 80rpx;
  text-align: center;
}

.bt_list .yy {
  background: #f66;
  color: #fff;
  font-size: 28rpx;
}

.bt_list .mf {
  background: #f66;
  color: #fff;
  width: 100%;
}

.table_form{
  height: auto;
  overflow: hidden;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  background: #fff;
  margin-top: 20rpx;
}

.table_form .input{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}

.table_form .input text{
  float: left;
  width: 20%;
  margin-left: 2%;
}
.table_form .input input{
  float: left;
  width: 70%;
  margin-left: 5rpx;
  height: 80rpx;font-size: 24rpx;
}
.table_form .input radio-group{
  float: left;margin-left: 5rpx;
  font-size: 24rpx; color: #666;
}
.table_form .input radio{
  transform:scale(0.8);
}

.table_form .photolist{
  height: auto;
  overflow: hidden;
  width: 96%;
  margin-left: 2%;
}
.table_form .photolist .title{
  width: 20%;
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
}
.table_form .photolist .action{
  width: 70%;
  float: left;margin-left: 10rpx;
  height: 100rpx;
}
.table_form .photolist .action image{
    height: 100rpx;
    width: 100rpx;
}
.table_form .photolist .photos{
  float: left;
  margin-top: 20rpx;
  width:100%;
  height: auto;
  overflow: hidden;
}
.table_form .photolist image{
  float: left;
  width: 120rpx;
  height: 120rpx;
  margin-right: 10rpx;
}
.table_form .picker{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}
.table_form .map{
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
}
.table_form .map .lt{
  float: left;
  width: 20%;
  margin-left: 2%;
  font-size: 26rpx;

}
.table_form .map .ct{
  width: 60%;
  height: 80rpx;
  overflow: hidden;
  float: left;
  margin-left: 5rpx;
  font-size: 24rpx;
}
.table_form .map image{
  width: 40rpx;
  height: 40rpx;
  float: right;margin-right: 10rpx;
  margin-top: 20rpx;
}

.table_form .picker .lt{
  float: left;
  width: 20%;
  margin-left: 2%;
}
.table_form .picker .ct{
  float: left;
  width: 60%;
  margin-left: 5rpx;
  font-size: 24rpx;
  color: #666
}

.table_form .picker .rt{
  float: right;
  margin-right: 10rpx; height: 40rpx;
  width: 40rpx;
  margin-top: 20rpx;
}

.table_btn{
  width: 60%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #fff;
  background:  #FF6666;
  margin-top: 30rpx;
}
.table_form .textarea{
  height: 200rpx;
  width: 96%;
  margin-left: 2%;
  font-size: 26rpx;
}

.table_form .textarea text{
  line-height: 200rpx;
  width: 20%;
  float: left;
}


.table_form .textarea textarea{
  width: 70%;
  margin-left: 5rpx;
  height: 150rpx;
  margin-top: 25rpx;
  float: left;
  border: 1rpx dotted   #f2f2f2;
  font-size: 24rpx;
  padding: 10rpx;
}

.table_form .logo{
  height: 100rpx;
  font-size: 26rpx;
}
.table_form .logo .lt{
  float: left;width: 20%;
  margin-left: 2%;
  height: 100rpx;
  line-height: 100rpx;
}

.table_form .logo image{
  float: left;
  margin-left: 5rpx;
  height: 100rpx;
  width: 100rpx;
}

.bdbt{
  border-bottom: 1rpx solid #e2e2e2;
}

.dwbg {
  height: 100%;
  width: 100%;
  background: #000;
  opacity: 0.5;
  position: fixed;
  top: 0;
  left: 0;
}

.qrcode_dialog {
  width: 80%;
  margin: 0 10%;
  height: 560rpx;
  background: #fff;
  z-index: 999;
  position: fixed;
  bottom: 250rpx;
  top: 250rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
  border-top: 1rpx #e2e2e2 solid;
  border-bottom: 1rpx #e2e2e2 solid;
  padding-bottom: 20rpx;
  -webkit-box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
}

.qrcode_dialog .qrcode_bg{
  width: 60%;
  margin: 10% 20%;
  height: 360rpx;
}

.qrcode_dialog .qrcode_bg image{
  width: 100%;
  height: 360rpx;
}

.qrcode_dialog .btn_save{
  width: 60%;
  height: 70rpx;
  background: #0a96cb;
  margin: 0 20%;
  color: #fff;
  font-size: 26rpx;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
