/**index.wxss**/

.main {
  width: 100%;
  height: auto;
  margin: 0 auto;
  overflow: hidden;
}

.main .banner {
  width: 100%;
  height: 380rpx;
  position: relative;
}

.main .banner image {
  width: 100%;
  height: 380rpx;
}

.main .banner .title {
  position: absolute;
  left: 30rpx;
  bottom: 20rpx;
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
}

.main .address {
  width: 92%;
  height: auto;
  padding: 30rpx 4%;
  background: #fff;
  font-size: 28rpx;
}

.main .menu {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 20rpx;
  background: #fff;
  border-bottom: solid 1rpx #f4f5f6;
}

.main .menu text {
  float: left;
  width: 33%;
  font-size: 30rpx;
  text-align: center;
}

.main .menu text.active {
  color: #07b7ce;
  border-bottom: 5rpx solid #07b7ce;
}

.main .date {
  width: 100%;
  height: 120rpx;
  padding-top: 2rpx;
  background: #fff;
  position: relative;
  border-bottom: solid 1rpx #f4f5f6;
  display: inline-block;
}

.main .date .rudian {
  width: 49.8%;
  height: 120rpx;
  float: left;
}

.main .date .ytitle {
  width: 100%;
  height: 30rpx;
  font-size: 28rpx;
  margin-top: 20rpx;
  text-align: center;
  color: #000;
}

.main .date .ydate {
  width: 100%;
  height: 32rpx;
  margin-top: 10rpx;
  font-size: 30rpx;
  text-align: center;
  color: #000;
}

.main .date .lidian {
  width: 49.8%;
  height: 120rpx;
  float: right;
}

.main .date .vline {
  width: 2rpx;
  height: 120rpx;
  background: #f5f5f5;
  float: left;
}

.main .date .count {
  width: 16%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 25rpx;
  border: 1rpx #f5f5f5 solid;
  position: absolute;
  background: #fff;
  overflow: hidden;
  top: 35rpx;
  left: 42%;
  font-size: 23rpx;
  color: #000;
}

.main .yuding {
  width: 100%;
  height: auto;
}

.main .list {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.main .list .detail {
  width: 100%;
  height: auto;
  background: #f5f5f5;
}

.intro {
  width: 94%;
  height: 120rpx;
  padding: 15rpx 3%;
  background: #f5f5f5;
}

.intro .lt {
  width: 69%;
  height: 120rpx;
  float: left;
  overflow: hidden;
}

.intro .lt .ititle {
  margin-top: 20rpx;
  font-size: 28rpx;
  width: 100%;
}

.intro .lt .idetail {
  width: 100%;
  height: auto;
}

.intro .lt .tv01 {
  width: auto;
  font-size: 26rpx;
  color: #515151;
}

.intro .lt .tv02 {
  width: auto;
  font-size: 26rpx;
  color: #515151;
  margin-left: 10rpx;
}

.intro .rt {
  width: 28%;
  height: 120rpx;
  float: right;
  padding-left: 15rpx;
  display: flex;
  align-items: center;
}

.intro .rt .iprice {
  color: #f00;
  font-size: 32rpx;
  float: left;
}

.intro .rt .idetails {
  width: 80rpx;
  height: auto;
  float: left;
  margin-left: 15rpx;
}

.intro .rt .idetails .dtv01 {
  width: 80rpx;
  height: 30rpx;
  text-align: center;
  background: #1c86ee;
  font-size: 24rpx;
  color: #fff;
}

.intro .rt .idetails .dtv02 {
  width: 80rpx;
  height: 40rpx;
  text-align: center;
  background: #fff;
  font-size: 24rpx;
  color: #1c86ee;
}

.intro .rt .ihint {
  color: #515151;
  font-size: 26rpx;
  float: left;
}

.main .list .item {
  width: 94%;
  height: 160rpx;
  padding: 15rpx 3%;
  background: #fff;
  border-bottom: 1rpx #f5f5f5 solid;
}

.main .list .item .lt {
  width: 28%;
  height: 160rpx;
  float: left;
}

.main .list .item .lt image {
  width: 100%;
  height: 100%;
}

.main .list .item .mt {
  width: 45%;
  height: 160rpx;
  padding: 0 15rpx;
  float: left;
}

.main .list .item .mt .ntitle {
  width: 100%;
  font-size: 28rpx;
  color: #000;
  line-height: 32rpx;
  margin-bottom: 15rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  float: left;
}

.main .list .item .mt .ndetail {
  font-size: 27rpx;
  line-height: 32rpx;
  color: #bdbdbd;
}

.main .list .item .rt {
  width: 20%;
  height: 160rpx;
  float: right;
  display: flex;
  align-items: center;
  padding-left: 15rpx;
}

.main .list .item .rt .price {
  color: #f00;
  font-size: 32rpx;
}

.main .list .item .rt image {
  width: 40rpx;
  height: 40rpx;
}

.main .list .item .rt .hint {
  color: #515151;
  font-size: 26rpx;
}

.main .jddetail {
  width: 92%;
  height: auto;
  padding: 0 4%;
  background: #fff;
  overflow: hidden;
  margin-bottom: 100rpx;
}

.main .dheader {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx #f5f5f5 solid;
  font-size: 28rpx;
  color: #8a8a8a;
}

.main .jddetail .dcontent {
  margin: 20rpx 0;
  font-size: 26rpx;
  line-height: 34rpx;
  color: #000;
}

.main .jddetail .tags {
  width: 100%;
  height: auto;
  margin: 15rpx 0;
}

.main .jddetail .tags .item {
  width: 28%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  background: #f5f5f5;
  border-radius: 10rpx;
  font-size: 26rpx;
  margin: 15rpx 15rpx;
  color: #fff;
  display: inline-block;
}

.margin_right_05 {
  margin-right: 5%;
}

.main .comment {
  width: 100%;
  height: auto;
  overflow: hidden;
}

