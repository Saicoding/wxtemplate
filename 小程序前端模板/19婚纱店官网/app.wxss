/**app.wxss**/
page {
  background: #f3f5f7;
  font-size: 24rpx;
}

.jszc{
  height: 80rpx;
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  color: #ccc;
}

.kongbai{
  width: 100%;
  height: 100rpx;
}

.footer {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top: 1rpx solid #e2e2e2;
  z-index: 10000;
}

.footer .ft_item {
  width: 25%;
  height: 100%;
  text-align: center;
  float: left;
  color: #666;
}

.footer .ft_item .image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
  font-size: 45rpx;
}

.footer .ft_item image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
}

.footer  .ft_item text {
  text-align: center;
  font-size: 26rpx;
  display: block;
  margin: 0 auto;
  margin-top: 10rpx;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.icon-guanyu:before { content: "\e6b6"; }

.icon-yuyue:before { content: "\e7cd"; }

.icon-zhaopian-copy:before { content: "\e63d"; }

.icon-xiangji1:before { content: "\e606"; }

