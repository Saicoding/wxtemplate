/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
  background: #f5f5f5;
}

.app {
  width: 100%;
  height: auto;
  overflow: hidden;
  font-size: 26rpx;
  background: #f5f5f5;
}

.dingzhi_footer {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fcfcfc;
}

.dingzhi_footer .ft_item {
  width: 33%;
  height: 100%;
  text-align: center;
  float: left;
}

.dingzhi_footer .on {
  color: #0067D8;
}

.dingzhi_footer .off {
  color: #dbdbdb;
}

.dingzhi_footer .ft_item image {
  height: 45rpx;
  width: 45rpx;
  margin: 8rpx auto;
  display: block;
}

.dingzhi_footer .ft_item text {
  font-family: "微软雅黑";
  font-size: 24rpx;
  color: #1ba8b9;
}

.dingzhi_footer .ft_item .off {
  font-family: "微软雅黑";
  font-size: 24rpx;
  color: #8a8a8a;
}

.green_color{
  color: #1ba8b9;
}

