/**app.wxss**/

page {
  background: #f5f5f5;
}

.app {
  width: 100%;
  height: auto;
  font-size: 26rpx;
  overflow: hidden;
}

.add {
  position: fixed;
  right: 20rpx;
  bottom: 150rpx;
  z-index: 1000;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 40rpx;
  justify-content: center;
  display: flex;
}

.add text {
  width: 40rpx;
  height: 40rpx;
  margin: 0 20rpx;
  float: left;
  font-size: 40rpx;
  color: #fff;
}

.jszc {
  font-size: 24rpx;
  margin-top: 50rpx;
  width: 100%;
  text-align: center;
  color: #ccc;
}

.slide-image {
  width: 100%;
}

.kongbai {
  height: 100rpx;
  width: 100%;
  margin-top: 20rpx;
}

.app .nodata {
  height: 400rpx;
  width: 100%;
}

.app .nodata image {
  height: 200rpx;
  width: 200rpx;
  display: block;
  margin: 0 auto;
  margin-top: 50rpx;
}

.app .nodata text {
  display: block;
  margin: 0 auto;
  margin-top: 20rpx;
  color: #1ba8b9;
  font-size: 28rpx;
  text-align: center;
}

.footer {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top: 1rpx solid #e2e2e2;
}

.footer .ft_item {
  width: 25%;
  height: 100%;
  text-align: center;
  float: left;
  color: #666;
}

.footer .ft_item .image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
  font-size: 45rpx;
}

.footer .ft_item image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
}

.footer  .ft_item text {
  text-align: center;
  font-size: 22rpx;
  display: block;
  margin: 0 auto;
  margin-top: 10rpx;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
         url('hinted-iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shangjia:before {
  content: "\e6bd";
}

.icon-icon-yxj-empty-my:before {
  content: "\e651";
}

.icon-dingwei:before {
  content: "\e6b0";
}

.icon-toutiao:before {
  content: "\e655";
}

