/**app.wxss**/

page {
  background: #f5f5f5;
  font-size: 26rpx;
}

.bdbt{
  border-bottom: 1rpx solid #e2e2e2;
}

.circle {
  width: 16rpx;
  height: 16rpx;
  border-radius: 8rpx;
  float: left;
  margin: 22rpx 10rpx;
}

.kongbai {
  width: 100%;
  height: 100rpx;
}

.jszc {
  height: 80rpx;
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  color: #ccc;
}

.more, .nomore {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  background: #fff;
  width: 100%;
  display: flex;
  justify-content: center;
  border-top: 1rpx #f5f5f5 solid;
}

.nomore {
  color: #ccc;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.footer {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top: 1rpx solid #e2e2e2;
  z-index: 10000;
}

.footer .ft_item {
  width: 33.3%;
  height: 100%;
  text-align: center;
  float: left;
  color: #666;
}

.footer .ft_item .image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
  font-size: 45rpx;
}

.footer .ft_item image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
}

.footer  .ft_item text {
  text-align: center;
  font-size: 26rpx;
  display: block;
  margin: 0 auto;
  margin-top: 10rpx;
}

@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-faxian:before { content: "\e609"; }

.icon-wode:before { content: "\e60f"; }

.icon-shouye:before { content: "\e61d"; }