/** 颜色值 **/

.black {
  color: black;
}

.whitle {
  color: white;
}

.cheng {
  color: #ff9000;
}

.gray {
  color: #b0b0b0;
}

/** 背景色 **/

bg_black {
  background: black;
}

bg_cheng {
  background: #ff9000;
}

bg_white {
  background: white;
}

bg_red {
  background: red;
}

/** 字体大小 **/

.font_12 {
  font-size: 12px;
}

.font_14 {
  font-size: 14px;
}

.font_16 {
  font-size: 16px;
}

.font_18 {
  font-size: 18px;
}

/*定位*/

.pr {
  position: relative;
}

.pa {
  position: absolute;
}

/* 文字超出省略 */

.ellipsis {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: block;
}

/* 对齐 */

.tl {
  text-align: left;
}

.tc {
  text-align: center;
}

.tr {
  text-align: right;
}

.bte {
  border-top: 1px solid #eee;
}

.bre {
  border-right: 1px solid #eee;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/** 查看图片 **/

.photoshow {
  height: 100%;
  width: 100%;
  overflow: hidden;
  position: fixed;
  z-index: 1000;
  top: 0;
  left: 0;
}

.photoshow .bg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: #000;
  opacity: 0.8;
}

.photoshow .sw {
  position: absolute;
  width: 100%;
  height: 500rpx;
  left: 0;
  top: 300rpx;
  z-index: 1002;
}

.photoshow .sw_item {
  display: block;
  height: 500rpx;
}

.photoshow .sw_item image {
  width: 100%;
  height: 500rpx;
}

.more {
  height: 60rpx;
  line-height: 60rpx;
  border-top: 1px solid #f3f3f3;
  text-align: center;
  font-size: 26rpx;
  color: #1ba8b9;
  background: #fff;
}

