} */

.main .score .sfen {
  font-size: 36rpx;
  color: #f00;
  margin-left: 10rpx;
}

.main .comment .cnum {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #8a8a8a;
  background: #fff;
  text-align: center;
}

.item_detail {
  width: 90%;
  margin: 0 5%;
  height: 60%;
  background: #fff;
  z-index: 999;
  bottom: 20%;
  top: 20%;
  border-radius: 4rpx;
  font-size: 28rpx;
  border-top: 1rpx #e2e2e2 solid;
  border-bottom: 1rpx #e2e2e2 solid;
  padding-bottom: 20rpx;
  -webkit-box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  overflow: hidden;
  position: fixed;
}

.item_detail .tops {
  width: 92%;
  padding: 0 4%;
  height: 160rpx;
  overflow: hidden;
  border-bottom: 1rpx #f5f5f5 solid;
}

.item_detail .tops .lt {
  width: 80%;
  height: 140rpx;
  padding-top: 20rpx;
  float: left;
}

.item_detail .tops .lt .title {
  width: 100%;
  height: auto;
  font-size: 30rpx;
  color: #000;
}

.item_detail .tops .lt .detail {
  width: 100%;
  height: auto;
  font-size: 26rpx;
  margin-top: 10rpx;
  color: #8a8a8a;
}

.item_detail .tops .rt {
  width: 20%;
  height: 140rpx;
  padding-top: 20rpx;
  float: right;
  text-align: right;
}

.item_detail .tops .rt image {
  width: 40rpx;
  height: 40rpx;
}

.item_detail .tags {
  width: 92%;
  padding: 0 4%;
  height: 90rpx;
  margin-top: 20rpx;
}

.item_detail .tags .item {
  width: 30%;
  height: 70rpx;
  background: #f5f5f5;
  border-radius: 10rpx;
  float: left;
  font-size: 28rpx;
  color: #218868;
  display: flex;
  align-items: center;
  justify-content: center;
}

.item_detail .xuzhi {
  width: 92%;
  padding: 0 4%;
  font-size: 28rpx;
  color: #f00;
  margin-top: 20rpx;
}

.item_detail .item_content {
  width: 92%;
  height: auto;
  padding: 30rpx 4%;
}

.item_detail .item_content .lt {
  width: 30%;
  height: auto;
  font-size: 26rpx;
  float: left;
}

.item_detail .item_content .rt {
  width: 70%;
  height: auto;
  font-size: 26rpx;
  float: left;
}

.item_detail .bview {
  position: fixed;
  bottom: 18%;
  width: 92%;
  margin: 0 4%;
  height: 80rpx;
  border-top: 1rpx #f5f5f5 solid;
}

.item_detail .bview .lb {
  width: 60%;
  height: 80rpx;
  float: left;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #f00;
}

.item_detail .bview .rb {
  width: 33%;
  height: 80rpx;
  float: left;
  background: #218868;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
}

.main .comment .detail {
  width: 92%;
  height: auto;
  margin-top: 20rpx;
  padding: 0 4%;
  background: #fff;
}

.main .comment .detail .tit {
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 10rpx;
}

.main .comment .detail .tit .lt {
  width: 60%;
  float: left;
}

.main .comment .detail .tit image {
  height: 80rpx;
  width: 80rpx;
  margin: 10rpx 0;
  border-radius: 10rpx;
  float: left;
}

.main .comment .detail .tit .lt text{
  font-size: 28rpx;
  margin-left: 10rpx;
}

.main .comment .detail .tit .rt {
  float: right;
  font-size: 28rpx;
  text-align: right;
}

.main .comment .detail .content {
  width: 96%;
  margin: 0 auto;
  line-height: 40rpx;
  margin-top: 10rpx;
  font-size: 28rpx;
  margin-bottom: 10rpx;
  height: auto;
  overflow: hidden;
}

.main .comment .detail .photo {
  width: 96%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
}

.main .comment .detail .photo image {
  float: left;
  width: 100rpx;
  height: 100rpx;
  margin-left: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
